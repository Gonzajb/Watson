@model WatsonORT.Presentacion.Models.IngresoCodigoViewModel
@{
    ViewBag.Title = "Resultado del Análisis";
    //Layout = "~/Views/Shared/_Layout.cshtml";
}

<h2>@ViewBag.Title.</h2>
@using (Html.BeginForm("IngresoCodigo", "ResultadoWatson"))
{
    @Html.AntiForgeryToken()
    <div class="form-horizontal">
        <h4>Ingrese su codigo de consulta</h4>
        <hr />
        @Html.ValidationSummary("", new { @class = "text-danger" })
        <div class="form-group">
            @Html.LabelFor(m => m.CodigoConsulta, new { @class = "col-md-2 control-label" })
            <div class="col-md-10">
                @Html.TextBoxFor(m => m.CodigoConsulta, new { @class = "form-control" })
            </div>
        </div>

        <div class="form-group">
            <div class="col-md-offset-2 col-md-10">
                <input type="submit" class="btn btn-default" value="Ingresar" />
            </div>
        </div>
    </div>
}
        <div>
            @Html.ActionLink("Volver", "Index","Home")
        </div>

        @section Scripts {
            @Scripts.Render("~/bundles/jqueryval")
        }


